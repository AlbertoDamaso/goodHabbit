{
    "version": 2,
    "builds": [
      {
        "src": "*.ts",
        "use": "@vercel/node-compiler",
        "config": {
          "typescript": {
            "compilerOptions": {
              "target": "es2016",
              "module": "commonjs",
              "lib": ["es2016", "dom"]
            }
          }
        }
      }
    ],
    "routes": [
      { "src": "/.*", "dest": "server.js" }
    ]
  }
  